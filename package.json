{
	"name": "askmy",
	"version": "0.0.0",
	"workspaces": [
		"packages/*",
		"services/*",
		"apps/*"
	],
	"packageManager": "bun@1.2.18",
	"author": {
		"email": "keanuharrell@icloud.com",
		"name": "Keanu Harrell",
		"url": "https://www.keanuharrell.com"
	},
	"scripts": {
		"dev": "sst dev",
		"deploy:dev": "sst deploy --stage dev",
		"deploy:prod": "sst deploy --stage prod",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"check": "tsc --noEmit",
		"check:circular": "madge --circular --extensions ts,tsx ./packages",
		"prepare": "husky"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.0",
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@tsconfig/node22": "^22",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.6",
		"madge": "^8.0.0",
		"typescript": "^5"
	},
	"dependencies": {
		"sst": "3.17.21"
	}
}
